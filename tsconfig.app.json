/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [ "node" ],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "node_modules/nitro-renderer/src/*.ts",
    "node_modules/nitro-renderer/src/**/*.ts",
    "src/main.ts",
    "src/polyfills.ts"
  ]
}
